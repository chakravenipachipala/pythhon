
class solution2:
    def palindrome(self,n):
        x = ''
        for i in range(len(n)-1,-1,-1):
            x+=n[i]
        if x == n :
            return True
        else:
            return False
n = input()
a = solution2()
z = a.palindrome(n)
if z == True:
    print('it is a palindrome')
else:
    print('it is not a palindrome')
