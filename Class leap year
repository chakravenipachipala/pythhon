class solution1:
    def leap_year(self,n):
        if n % 400 ==0 and n % 100 == 0:
            return True
        elif n % 4 == 0 and n % 100 != 0:
            return True
        else:
            return False
n = int(input())
a = solution1()
z = a.leap_year(n)
if z == True:
    print('it is a leap year')
else:
    print('it is not a leap year')
